# add following to .zshrc
#
# source zshrc.landtax
#
# and create a symlink to this file

# landtax theme needs to be copied to themes folder first
ZSH_THEME="landtax"

plugins=(osx jira history fancy-ctrl-z docker-compose dirhistory bgnotify git branch bundler colored-man-pages common-aliases dircycle)

export JIRA_URL='https://xxx.atlassian.net'
export JIRA_NAME='landtax'
export JIRA_PREFIX='XXX-'
export JIRA_RAPID_BOARD=true #` - Set to `true` if you use Rapid Board
export JIRA_DEFAULT_ACTION='dashboard' #` - Action to do when `jira` is called with no arguments; defaults to "new"

export BUNDLER_EDITOR=mvim

alias gg="git status"
alias gc="git commit"

alias be="bundle exec"

source ~/dotfiles/tmuxinator.zsh
export EDITOR='mvim'
